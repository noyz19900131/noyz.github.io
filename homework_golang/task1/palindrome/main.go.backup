/*
题目：判断一个整数是否是回文数 。回文数是指正序（从左向右）和倒序（从右向左）读都是一样的整数。
*/
package main

import (
	"fmt"
	"strconv"
)

func main() {
	for i := 0; i < 1000000000; i++ {
		if isPalindrome(i) {
			fmt.Println(i)
		}
	}
}

func isPalindrome(x int) bool {
	if x < 0 {
		return false
	}
	str := strconv.Itoa(x)
	for i := 0; i < len(str)/2; i++ {
		if str[i] != str[len(str)-1-i] {
			return false
		}
	}
	return true
}
