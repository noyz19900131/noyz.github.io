/*
给定一个由整数组成的非空数组所表示的非负整数，在该数的基础上加一
*/
package main

import "fmt"

func main() {
	digits := []int{9, 9, 9}
	fmt.Println(plusOne(digits))
}

func plusOne(digits []int) []int {
	for i := len(digits) - 1; i >= 0; i-- {
		if digits[i] < 9 {
			digits[i]++
			return digits
		}
		digits[i] = 0
	}
	return append([]int{1}, digits...) // 如果所有位都是9，则需要在最高位加1
}
